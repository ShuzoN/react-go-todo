
{
  "name": "headphonista",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "web",
  "settings": { 
    "terminal.integrated.shell.linux": "/bin/bash",
    "go.gopath": "/go",
    "go.inferGopath": true,
    "go.useLanguageServer": true
  },
  "remoteUser": "root",
  "workspaceFolder":"/go/src/app",
  "extensions": [
    "ms-vscode.go"
  ]
}
